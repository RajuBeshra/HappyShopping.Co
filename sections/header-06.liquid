                                        --color_text: {{ text_color }};
                                        --background_color_text: {{ text_bg }};
                                        --color_text_active: {{text_color_active}};
                                        --background_color_text_active: {{ text_bg_active }};
                                        --padding-vertical: {{ padding_vertical }};
                                        --padding-horizontal: {{ padding_hozi }};
                                        --padding-vertical-mb: {{ padding_vertical_mb }};
                                        --padding-horizontal-mb: {{ padding_hozi_mb }};
