                            assign margin_bottom_sub_title = block.settings.margin_bottom_sub_title
                            assign margin_bottom_sub_title_item_2 = block.settings.margin_bottom_sub_title_item_2
                            assign margin_bottom_heading = block.settings.margin_bottom_heading
                            assign margin_bottom_des = block.settings.margin_bottom_des
                            assign margin_bottom_des_item_2 = block.settings.margin_bottom_des_item_2
    
                            assign countdown = block.settings.countdown
                            assign font_size_countdown_number = block.settings.font_size_countdown_number
                            assign font_size_countdown_text = block.settings.font_size_countdown_text
                            assign color_countdown = block.settings.color_countdown
                            assign color_countdown_mb = block.settings.color_countdown_mb
                            assign countdown_margin_bottom = block.settings.countdown_margin_bottom
                            assign hide_countdown_fc = block.settings.hide_countdown_fc
