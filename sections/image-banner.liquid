                            assign bg_color_icon_item_2 = block.settings.bg_color_icon_item_2 | color_modify: 'alpha', 0.4
                            assign adaptive_height_video = block.settings.adaptive_height_video
                            assign adaptive_height_video_item_2 = block.settings.adaptive_height_video_item_2
