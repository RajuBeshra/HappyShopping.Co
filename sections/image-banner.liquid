                                                {% if enable_custom_video_icon_position %}  
                                                    style="
                                                        --left: {{ video_icon_horizontal_position | append: '%' }};
                                                        --top: {{ video_icon_vertical_position | append: '%' }};
                                                        --left-mb: {{ video_icon_horizontal_position_mb | append: '%' }};
                                                        --top-mb: {{ video_icon_vertical_position_mb | append: '%' }};
                                                        "       
                                                {% endif %}
                                            >
                                                <button type="button" class="video-button" data-type="{{ video_link.type }}" 
                                                    data-src="{{ video_link.id }}" 
                                                    aspect_ratio="{{ adaptive_height_video| default: 56 | append: '%' }}" 
                                                    style="--color: {{ color_icon }};
                                                        {% if block_banner.settings.bg_color_icon != 'rgba(0,0,0,0)' and block_banner.settings.bg_color_icon != blank %}
                                                            --bg_color: {{ bg_color_icon }};
                                                        {% endif %}">
                                                    {%- if icon_style == 'style_2' -%}
