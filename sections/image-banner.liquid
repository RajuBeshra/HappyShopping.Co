                            assign enable_scroll_down = block.settings.enable_scroll_down
    
                            assign enable_max_width_image = block.settings.enable_max_width_image
                            assign custom_width_image = block.settings.custom_width_image
    
                            assign row_reverse = block.settings.row_reverse
                            assign row_reverse_item_2 = block.settings.row_reverse_item_2
                            assign width_banner = block.settings.width_banner | append: '%'
                            assign display_countdown = block.settings.display_countdown
                            assign title = block.settings.title
                            assign des = block.settings.des
                            assign countdown = block.settings.countdown
                            assign title_font_size = block.settings.title_font_size
                            assign title_font_size_mb = block.settings.title_font_size_mb
                            assign title_font_weight = block.settings.title_font_weight
