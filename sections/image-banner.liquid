                                    <div class="img-box img-box--mobile{% if enable_parallax_image %} is-parallax{% endif %}">
                                        {%- if enable_parallax_image -%}
                                            <a class="image--parallax"{% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %}>
                                                <div class="parallax-image" style="background-image: url({{ preview_img | img_url: 'master' }});"></div>
                                            </a>
                                        {%- endif -%}
                                        {%- if image != blank -%}
                                            <a class="image image-adapt adaptive_height"{% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: image.aspect_ratio | times: 100 | append: '%' }}" title="{{ preview_img.alt | default: shop.name | escape }}">
                                                <img {% render 'lazyload', preview_img: preview_img %} loading="lazy" />
                                                {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                            </a>
                                        {%- else -%}
                                            <a class="image"{% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %}>
                                                <div class="not_img" style="--height_not_img: 720px">
                                                    1880 x 720px
                                                </div>  
                                            </a>
                                        {%- endif -%}
                                        {%- if mobile_image != blank -%}
                                            <a class="image image-mobile image-adapt adaptive_height"{% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %} {% if link == blank %} role="link" aria-disabled="true"{% else %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: mobile_image.aspect_ratio | times: 100 | append: '%' }}" title="{{ mobile_image.alt | default: shop.name | escape }}">
                                                <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{{ mobile_image | img_url: 'master' }} 1x, {{ mobile_image | img_url: 'master', scale: 2 }} 2x" {% unless settings.enable_lazyload %}src="{{ mobile_image | img_url: 'master' }}"{% endunless %} alt="{{ mobile_image.alt | default: shop.name | escape }}" loading="lazy" 
                                                {% if settings.enable_lazyload %}class="lazyload"{% endif %}>
                                                {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                            </a>
                                        {%- else -%}
