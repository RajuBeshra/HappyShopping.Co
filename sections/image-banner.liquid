                                                    <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{{ image | image_url: width: 420, crop: 'center' }} 1x, {{ image | image_url: width: 420, crop: 'center',  scale: 2 }} 2x" {% unless settings.enable_lazyload %}src="{{ image | img_url: '420x', crop: 'center' }}"{% endunless %} alt="{{ image.alt | default: shop.name | escape }}" loading="lazy" 
                                                    {% if settings.enable_lazyload %}class="lazyload"{% endif %}>
                                                    {% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}
                                                </a>
                                            {%- else -%}
