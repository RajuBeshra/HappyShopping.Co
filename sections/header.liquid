                            {%- if section.settings.show_cart -%}
                                <a href="{{ routes.cart_url }}" class="header__icon header__icon--cart link link--text focus-inset cart-icon-bubble" {% if settings.show_quick_cart %} data-cart-sidebar{% endif %}>
                                    {% render 'icon-cart-02' %}
                                    <span class="cart-text">{{ 'general.cart.shopping_cart' | t }}</span>
                                    <div class="cart-count-bubble">
                                        {%- if cart.item_count < 100 -%}
                                            <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                        {%- endif -%}
                                        <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                    </div>
                                </a>
                            {%- endif -%}
                            {%- if settings.show_wishlist and section.settings.show_wishlist -%}
                                <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link link--text focus-inset">
                                    {% render 'icon-heart-2' %}
                                    <span class="wishlist-text">
                                        {{ 'wishlist.general.title' | t }}
                                    </span>
                                </a>
                            {%- endif -%}
                            {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                                <div class="customer-links">
                                  {% if customer %}
                                      <a class="header__link--account" id="customer_logout_link" href="{{ routes.account_logout_url }}">
                                        {{ 'customer.log_out' | t }}
                                      </a>
                                      <span> / </span>
                                      <a class="header__link--account" href="{{ routes.account_url }}">
                                        {{ 'customer.account.my_account' | t }}
                                      </a>
                                  {% else %}
                                        {% if routes.account_login_url contains '/account/login' %}
                                            {% assign is_contained = true %}
                                        {% else %}
                                            {% assign is_contained = false %}
                                        {% endif %}
                                        <a class="header__link--account customer_login_link" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}" {% if is_contained %}data-open-auth-sidebar{% endif %}>
                                            {{ 'customer.sign_in' | t }}
                                        </a>
                                        {% if is_contained %}
                                            <span class="or-text">
                                                {{ 'customer.or' | t }}
                                            </span>
                                            <a class="header__link--account customer_register_link" href="{{ routes.account_register_url }}">
                                                {{ 'customer.create_account' | t }}
                                            </a>
                                        {% endif %}
                                  {% endif %}                                   
                                </div>
                            {%- endif -%}
                        </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <div class="header__logo header__logo_fixed fixed-left">
                <div class="header__heading">
                    <a href="{{ routes.root_url }}" class="header__heading-link focus-inset">
                        {%- if section.settings.logo_fixed != blank -%}
                            {%- assign logo = section.settings.logo_fixed -%}
                            {%- assign logo_width = section.settings.logo_width_fixed -%}
                            {%- assign logo_width_img = logo_width | append: 'x' -%}
                            {%- assign logo_width_retina = logo_width | times: 2 | append: 'x'  -%}
                            {%- assign logo_height = logo_width | divided_by: logo.aspect_ratio | round -%}
                            <img srcset="{{ logo | img_url: logo_width_img }}, {{ logo | img_url: logo_width_retina }} 2x"
                                src="{{ logo | img_url: logo_width_img }}" loading="lazy" class="header__heading-logo" alt="{{ logo.alt | default: shop.name | escape }}"
                                width="{{ logo_width }}" height="{{ logo_height }}">
                        {%- endif -%}
                    </a>
