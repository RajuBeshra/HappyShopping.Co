{% render 'header-04-style' %}

<script src="{{ 'details-disclosure.js' | asset_url }}" defer="defer"></script>
{% if settings.enable_layout_rtl %}
  <style type="text/css" media="screen">
    @media (min-width: 1025px) {
        .layout_rtl .header--left .header-top--wrapper .header-top--left {padding-left: 0; padding-right: 35px;}
        .layout_rtl .header:not(.style_3) .header-top--wrapper .header-top--right .header__icon--cart .cart-count-bubble {left: 0; right: auto}
        .layout_rtl .header-top--right .header__icon--wishlist .wishlist-text {width: auto}
        .layout_rtl .header-top--right .header__icon svg {margin-right: 0; margin-left: 8px}
        .layout_rtl .header-top--right .header__icon--account svg {margin-left: 8px;margin-right: 0}
        .layout_rtl .header-top--right .header__icon+.header__icon {margin-right: 30px;margin-left: 0}
        .layout_rtl .top-language-currency .halo-top-language+.halo-top-currency {margin-right: 1px;padding-right: 8px;margin-left: 0;padding-left: 0}
        .layout_rtl .top-language-currency.enable-currency .halo-top-language .disclosure__button .text {margin-left: 0;margin-right: 10px}
        .layout_rtl .top-language-currency .halo-top-language+.halo-top-currency:before {left: auto;right: -3px}
        .layout_rtl .top-language-currency .halo-top-language .disclosure__button {padding-left: 0}
        .layout_rtl .header-language_currency .top-language-currency .dropdown-currency{left: 0;right: auto}
        .layout_rtl .header-language_currency .top-language-currency.enable-currency .dropdown-language {left: 42px;right: auto}
        .layout_rtl .header-bottom-vertical .header__menu-vertical .list-menu .menu-lv-item.menu-lv-1 .menu-lv-1__action .icon {right: auto;left: 23px}
		.layout_rtl .header__menu-vertical .list-menu .menu-lv-item .list-menu--disclosure {left: auto;right: 100%}
     	.layout_rtl .header-bottom-right .customer-service-text {margin-right: 0;margin-left: 35px}
		.layout_rtl .header-bottom-vertical+.header-bottom-left {padding-left: 0;padding-right: 30px}
		.layout_rtl .header-bottom-vertical .categories-title svg {right: auto;left: 25px}
        .layout_rtl .header-bottom-vertical .categories-title .mobileMenu-toggle {margin-right: 0;margin-left: 16px} 
        .layout_rtl .header__search .search-modal__form .search__button {border-radius: 5px 0 0 5px}
        .layout_rtl .header-top--wrapper .quickSearchResultsWrap{left: auto; right: 0}
        .layout_rtl .style_2 .header-top-custom .info-store .text:first-child {padding-right: 0}
        .layout_rtl .style_2 .header-top-custom .info-icon {padding-right: 0;padding-left: 5px}
        .layout_rtl .style_2 .header-top-custom .header-links .link {margin-right: 0;margin-left: 20px}
        .layout_rtl .style_2 .header-top--right .header__icon--account svg {margin-right: 0;margin-left: 8px}
        .layout_rtl .style_2 .header-top-custom .top-language-currency.enable-currency .halo-top-language .disclosure__button .text {margin-right: 7px;margin-left: 0}
        .layout_rtl .style_2 .header-top-custom .top-language-currency .halo-top-language+.halo-top-currency {margin-left: 0;margin-right: 1px;padding-left: 0;padding-right: 5px}
        .layout_rtl .style_2 .top-language-currency .halo-top-language .disclosure__button {padding-left: 0}
        .layout_rtl .style_2 .header-top--right .header__icon--cart .cart-count-bubble {right: auto;left: -3px}
        .layout_rtl .style_2 .header__search .search-modal__form .search__button {border-radius: 5px 0 0 5px}
        .layout_rtl .style_2 .header-top--right .header__icon+.header__icon.header__icon--account {margin-right: 33px;margin-left: 0}
        .layout_rtl .quickSearchResultsBlock .productEmpty svg {transform: rotate(180deg)}
        .layout_rtl .style_2 .header-top--right .header__icon+.header__icon {margin-right: 30px;margin-left: 0}
    	.layout_rtl .style_2 .header-bottom-vertical+.header-bottom-left {padding-left: 0;padding-right: 20px}
        .layout_rtl .style_2 .header-top--wrapper .header__search .search-modal__form .search__input,.layout_rtl .style_2 .header-top--right .customer-links{text-align: right}
      	.layout_rtl .style_3 .header-bottom-vertical .categories-title .title {margin-right: 10px;margin-left: 0}
        .layout_rtl .style_3 .header-bottom-vertical .categories-title:before,.layout_rtl .style_3 .header-bottom-right .help-item:before {left: 0;right: auto}
        .layout_rtl .style_3 .header-top--wrapper .header__search .search-modal__form {border-radius: 31px 30px 30px 30px}
        .layout_rtl .style_3 .header__search .search-modal__form .search__button {border-radius: 30px 0 0 30px}
    	.layout_rtl .style_3 .header-bottom-vertical .categories-title svg {right: auto;left: 62px;margin-top: 0}
        .layout_rtl .style_3 .header-top--right .header__icon+.header__icon {margin-right: 40px;margin-left: 0}
		.layout_rtl .style_3 .header-bottom-vertical .categories-title .mobileMenu-toggle-2 svg {right: 0}
		.layout_rtl .style_3 .header-bottom-vertical+.header-bottom-left {padding-right: 22px;padding-left: 0}
        .layout_rtl .style_3 .header-bottom-right .help-item {margin-left: 14px;padding-right: 0;margin-right: 0;padding-left: 20px}
        .layout_rtl .header--center.style_3 .customer-service-text {padding-left: 0;padding-right: 30px;text-align: right}
    }
  </style>
{% endif %}
<{% if section.settings.enable_sticky_header %}sticky-header{% else %}div{% endif %} class="header-wrapper" data-hide-on-scroll={{ section.settings.hide_scroll }}>
    <header class="header header-04 header--{{ section.settings.logo_position }}{% if section.settings.menu != blank %} header--has-menu{% endif %}{% if section.settings.header_layout_style == 'style_2' %} style_2{% elsif section.settings.header_layout_style == 'style_3' %} style_3{% endif %}" style="--icon-mobile-spacing: {{ section.settings.header_icon_mobile_spacing | append: 'px' }};">
        {% if section.settings.header_layout_style == 'style_2' %}
        <div class="header-top-custom">
            <div class="container">
                <div class="header-top-custom-wrapper">
                    <div class="header-top-left text-left">
                       <ul class="info-store">
                            {% if section.settings.info_text_1 != blank %}
                            <li class="text">
                                {% if section.settings.info_icon_1 != blank %}
                                <span class="info-icon">
                                    {{ section.settings.info_icon_1 }}
                                </span>
                                {% endif %}

                                <div class="info-text">
                                    {{ section.settings.info_text_1 }}
                                </div>
                            </li>
                            {% endif %}
                            
                            {% if section.settings.info_text_2 != blank %}
                            <li class="text">
                                {% if section.settings.info_icon_2 != blank %}
                                <span class="info-icon">
                                    {{ section.settings.info_icon_2 }}
                                </span>
                                {% endif %}
                                <div class="info-text">
                                    {{ section.settings.info_text_2 }}
                                </div>
                            </li>
                            {% endif %}
                        </ul>
                    </div>
                    <div class="header-top-center text-center">
                        {% if section.settings.free_shipping_text != blank %}
                        <div class="free-shipping-text">
                            {{ section.settings.free_shipping_text }}
                        </div>
                        {% endif %}
                    </div>
                    <div class="header-top-right text-right">
                        {% if section.settings.header-link_1_text != blank or section.settings.header-link_2_text != blank %}
                            <ul class="header-links">
                                {% if section.settings.header-link_1_text != blank %}
                                <li class="link">
                                    <a {% if section.settings.header-link_1 == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.header-link_1 }}"{% endif %}>
                                        {{ section.settings.header-link_1_text }}
                                    </a>
                                </li>
                                {% endif %}
            
                                {% if section.settings.header-link_2_text != blank %}
                                <li class="link">
                                    <a {% if section.settings.header-link_2 == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.header-link_2 }}"{% endif %}>
                                        {{ section.settings.header-link_2_text }}
                                    </a>
                                </li>
                                {% endif %}
                            </ul>
                        {% endif %}
                        <div class="header-language_currency">
                            <div class="top-language-currency{% if settings.enable_currencies %} enable-currency{% endif %}">
                                {% if settings.enable_multilang_shopify %}
                                    {% render 'halo-top-language' %}
                                {% endif %}

                                {% if settings.enable_currencies %}
                                    {% render 'halo-top-currency' %}
                                {% endif %}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {% endif %}
        <div class="header-top">
            <div class="container">
                <div class="header-top--wrapper">
                  {% if section.settings.logo_position == 'left' %}
                    <div class="header-top--left header__logo text-left clearfix">
                        {%- if request.page_type == 'index' -%}
                            <h1 class="header__heading">
                        {%- else -%}
                            <div class="header__heading">
                        {%- endif -%}
                            <a href="{{ routes.root_url }}" class="header__heading-link focus-inset">
                                {%- if section.settings.logo != blank -%}
                                    {%- assign image = section.settings.logo -%}
                                    {%- assign image_size = section.settings.logo_width | append: 'x' -%}
                                    {%- assign logo_height = section.settings.logo_width | divided_by: section.settings.logo.aspect_ratio -%}
                                    {{ section.settings.logo | image_url: width: section.settings.width | image_tag:
                                        class: 'header__heading-logo',
                                        loading: 'lazy',
                                        widths: '50, 100, 150, 200, 250, 300, 400, 500',
                                        height: logo_height,
                                        width: section.settings.logo_width,
                                        alt: image.alt | default: shop.name | escape
                                      }}
                                {%- elsif section.settings.logo_text != blank -%}
                                    <span class="h2 {{ section.settings.logo_text_transform }}">{{ section.settings.logo_text }}</span>
                                {%- else -%}
                                    <span class="h2 {{ section.settings.logo_text_transform }}">{{ shop.name }}</span>
                                {%- endif -%}
                            </a>
                        {%- if request.page_type == 'index' -%}
                            </h1>
                        {%- else -%}
                            </div>
                        {%- endif -%}

                        {%- if section.settings.header_layout_style == 'style_3' -%}
                            {% if section.settings.display_customer_service and section.settings.customer_service_text != blank %}
                              <div class="customer-service-text">
                                {{ section.settings.customer_service_text }}
                              </div>
                            {% endif %}
                        {%- endif -%}
                    </div>
                  {% endif %}
                    <div class="{% if section.settings.logo_position == 'left' %}header-top-center text-center header__search{% else %}header-top--left text-left header__search{% endif %} clearfix">
                        {% if section.settings.show_search %}
                            <details-modal class="header__iconItem header__search">
                                {%- if settings.show_quick_search -%}
                                    <predictive-search class="search-modal__form" data-loading-text="{{ 'accessibility.loading' | t }}" data-product-to-show="{{ settings.quick_search_to_show }}">
                                {%- endif -%}
                                <details class="search_details">
                                    <summary class="header__search-full" aria-haspopup="dialog" aria-label="{{ 'general.search.search' | t }}">
                                        <button type="button" class="header-search-close" aria-label="{{ 'general.common.close' | t }}">
                                            {% render 'icon-close' %}
                                        </button>
                                        <form action="{{ routes.search_url }}" method="get" role="search" class="search search-modal__form">
                                            <div class="field">
                                                <input 
                                                    class="search__input field__input form-input-placeholder" 
                                                    id="Search-In-Modal" 
                                                    type="search" 
                                                    name="q" 
                                                    value="{{ search.terms | escape }}" 
                                                    placeholder="{{ 'general.search.search_the_store' | t }}"
                                                    autocomplete="off"
                                                    {%- if settings.show_quick_search -%}
                                                        role="combobox"
                                                        aria-expanded="false"
                                                        aria-owns="predictive-search-results-list"
                                                        aria-controls="predictive-search-results-list"
                                                        aria-haspopup="listbox"
                                                        aria-autocomplete="list"
                                                        autocorrect="off"
                                                        autocomplete="off"
                                                        autocapitalize="off"
                                                        spellcheck="false"
                                                    {%- endif -%}
                                                >   
                                                <input type="hidden" name="options[prefix]" value="last">
                                                <input type="hidden" name="type" value="product">
                                                <label class="field__label hiddenLabels" for="Search-In-Modal">
                                                    {{ 'general.search.search_the_store' | t }}
                                                </label>
                                                <button class="button search__button field__button focus-inset" aria-label="{{ 'general.search.search' | t }}">
                                                    {% render 'icon-search-02' %}
                                                </button>
                                            </div>
                                            {%- if settings.show_search_trending or settings.show_search_product -%}
                                                {%- render 'quick-searching-trending-products' -%}
                                            {%- endif -%}
                                            {%- if settings.show_quick_search -%}
                                                {%- render 'predictive-search-results' -%}
                                            {%- endif -%}
                                        </form>
                                    </summary>

                                    <div class="search-modal modal__content{% if settings.show_quick_search %} quickSearch{% endif %}" role="dialog" aria-modal="true" aria-label="{{ 'general.search.search' | t }}">
                                        {%- if settings.show_quick_search -%}
                                            {% render 'halo-quick-search' %}
                                        {%- endif -%}
                                    </div>
                                </details>
                                {%- if settings.show_quick_search -%}
                                    </predictive-search>
                                {%- endif -%}
                            </details-modal>
                        {% endif %}
                        {% if section.settings.logo_position == 'center' %}
                         {%- if section.settings.header_layout_style == 'style_3' -%}
                            {% if section.settings.display_customer_service and section.settings.customer_service_text != blank %}
                              <div class="customer-service-text">
                                {{ section.settings.customer_service_text }}
                              </div>
                            {% endif %}
                        {%- endif -%}
                       {% endif %}
                    </div>
                  {% if section.settings.logo_position == 'center' %}
                    <div class="header-top-center header__logo text-center clearfix">
                        {%- if request.page_type == 'index' -%}
                            <h1 class="header__heading">
                        {%- else -%}
                            <div class="header__heading">
                        {%- endif -%}
                            <a href="{{ routes.root_url }}" class="header__heading-link focus-inset">
                                {%- if section.settings.logo != blank -%}
                                    {%- assign image = section.settings.logo -%}
                                    {%- assign image_size = section.settings.logo_width | append: 'x' -%}
                                    {%- assign logo_height = section.settings.logo_width | divided_by: section.settings.logo.aspect_ratio -%}
                                    {{ section.settings.logo | image_url: width: section.settings.width | image_tag:
                                        class: 'header__heading-logo',
                                        loading: 'lazy',
                                        widths: '50, 100, 150, 200, 250, 300, 400, 500',
                                        height: logo_height,
                                        width: section.settings.logo_width,
                                        alt: image.alt | default: shop.name | escape
                                      }}
                                {%- elsif section.settings.logo_text != blank -%}
                                    <span class="h2 {{ section.settings.logo_text_transform }}">{{ section.settings.logo_text }}</span>
                                {%- else -%}
                                    <span class="h2 {{ section.settings.logo_text_transform }}">{{ shop.name }}</span>
                                {%- endif -%}
                            </a>
                        {%- if request.page_type == 'index' -%}
                            </h1>
                        {%- else -%}
                            </div>
                        {%- endif -%}
                    </div>
                  {% endif %}
                    <div class="header-top--right header__icons text-right clearfix">
                        {% if section.settings.show_cart or section.settings.show_wishlist or section.settings.show_customer %}
                        <div class="header__iconItem header__group">
                            {% if section.settings.header_layout_style == 'style_2' %}
                                {%- if section.settings.show_cart -%}
                                    <a href="{{ routes.cart_url }}" class="cart-icon-bubble header__icon header__icon--cart link link--text focus-inset"{% if settings.show_quick_cart %} data-cart-sidebar{% endif %}>
                                        {% render 'icon-cart-03' %}
                                        <span class="cart-text">{{ 'general.cart.cart' | t }}</span>
                                        <div class="cart-count-bubble">
                                            {%- if cart.item_count < 100 -%}
                                                <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                            {%- endif -%}
                                            <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                        </div>
                                    </a>
                                {%- endif -%}
                            {% else %}
                                {%- if section.settings.header_layout_style == 'style_3' -%}
                                    {%- if settings.show_wishlist and section.settings.show_wishlist -%}
                                        <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link link--text focus-inset">
                                            {% render 'icon-heart-2' %}
                                            <span class="wishlist-text">
                                                {{ 'wishlist.general.wishlist' | t }}
                                            </span>
                                        </a>
                                    {%- endif -%}
                                {%- endif -%}
                                {%- unless section.settings.header_layout_style == 'style_3' -%}
                                    {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                                        {% if routes.account_login_url contains '/account/login' %}
                                            {% assign is_contained = true %}
                                        {% else %}
                                            {% assign is_contained = false %}
                                        {% endif %}
                                        <a href="{{ routes.account_login_url }}" class="header__icon header__icon--account link link--text"{% if is_contained %} data-open-auth-sidebar{% endif %}>
                                            {% render 'icon-account-2' %}
                                            <span class="customer-links">
                                                <span class="hello">{{ 'customer.hello' | t }}</span>
                                                {%- liquid
                                                if customer
                                                    echo 'customer.account_fallback' | t
                                                else
                                                    echo 'customer.sign_in' | t
                                                endif
                                                -%}
                                            </span>
                                        </a>                               
                                    {%- endif -%}
                                {%- endunless -%}
                            {% endif %}

                            {%- if section.settings.header_layout_style == 'style_3' -%}
                                {%- if section.settings.show_giftcards -%}
                                    <a {% if section.settings.link_giftcards == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.link_giftcards }}"{% endif %} class="header__icon header__icon--giftcards link link--text focus-inset">
                                        {% render 'icon-giftcards' %}
                                        <span class="giftcards-text">
                                            {{ section.settings.text_giftcards }}
                                        </span>
                                    </a>
                                {%- endif -%}
                            {%- endif -%}

                            {%- unless section.settings.header_layout_style == 'style_3' -%}
                                {%- if settings.show_wishlist and section.settings.show_wishlist -%}
                                    <a href="{{ pages['wish-list'].url }}" class="header__icon header__icon--wishlist link link--text focus-inset">
                                        {% render 'icon-heart-2' %}
                                        <span class="wishlist-text">
                                            {{ 'wishlist.general.wishlist' | t }}
                                        </span>
                                    </a>
                                {%- endif -%}
                            {%- endunless -%}

                            {%- if section.settings.header_layout_style == 'style_3' -%}
                                {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                                    {% if routes.account_login_url contains '/account/login' %}
                                        {% assign is_contained = true %}
                                    {% else %}
                                        {% assign is_contained = false %}
                                    {% endif %}
                                    <a href="{{ routes.account_login_url }}" class="header__icon header__icon--account link link--text"{% if is_contained %} data-open-auth-sidebar{% endif %}>
                                        {% render 'icon-account-2' %}
                                        <span class="customer-links">
                                            {%- liquid
                                            if customer
                                                echo 'customer.account_fallback' | t
                                            else
                                                echo 'customer.sign_in' | t
                                            endif
                                            -%}
                                        </span>
                                    </a>                               
                                {%- endif -%}
                            {%- endif -%}

                            {% if section.settings.header_layout_style == 'style_2' %}
                                {%- if shop.customer_accounts_enabled and section.settings.show_customer -%}
                                    {% if routes.account_login_url contains '/account/login' %}
                                        {% assign is_contained = true %}
                                    {% else %}
                                        {% assign is_contained = false %}
                                    {% endif %}
                                    <a href="{{ routes.account_login_url }}" class="header__icon header__icon--account link link--text"{% if is_contained %} data-open-auth-sidebar{% endif %}>
                                        {% render 'icon-account-2' %}
                                        <div class="customer-links">

                                            {% if customer %}
                                                  <a class="header__link--account" id="customer_logout_link" href="{{ routes.account_logout_url }}">
                                                    {{ 'customer.log_out' | t }}
                                                  </a>
                                                  <span> / </span>
                                                  <a class="header__link--account" href="{{ routes.account_url }}">
                                                    {{ 'customer.account.my_account' | t }}
                                                  </a>
                                            {% else %}  
                                                  <a class="header__link--account" id="customer_login_link" href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"{% if is_contained %} data-open-auth-sidebar{% endif %}>
                                                    {{ 'customer.login_page.title' | t }}
                                                  </a>
                                                  <span class="or-text">
                                                    {{ 'customer.or' | t }}
                                                  </span>
                                                  <a class="header__link--account" id="customer_register_link" href="{{ routes.account_register_url }}">
                                                    {{ 'customer.login_page.register' | t }}
                                                  </a>
                                             {% endif %}          

                                             <span class="hello">{{ 'customer.my_account' | t }}</span>

                                        </div>
                                    </a>                               
                                {%- endif -%}
                            {% else %}
                                {%- if section.settings.show_cart -%}
                                    <a href="{{ routes.cart_url }}" class="cart-icon-bubble header__icon header__icon--cart link link--text focus-inset"{% if settings.show_quick_cart %} data-cart-sidebar{% endif %}>
                                        {% render 'icon-cart-03' %}
                                        <span class="cart-text">{{ 'general.cart.cart' | t }}</span>
                                        <div class="cart-count-bubble">
                                            {%- if cart.item_count < 100 -%}
                                                <span class="text" aria-hidden="true" data-cart-count>{{ cart.item_count }}</span>
                                            {%- endif -%}
                                            <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
                                        </div>
                                    </a>
                                {%- endif -%}
                            {% endif %}

                        </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <div class="header__logo header__logo_fixed fixed-left">
                <div class="header__heading">
                    <a href="{{ routes.root_url }}" class="header__heading-link focus-inset">
                        {%- if section.settings.logo_fixed != blank -%}
                            {%- assign image = section.settings.logo_fixed -%}
                            {%- assign image_size = section.settings.logo_width_fixed | append: 'x' -%}
                            <img srcset="{{ image | img_url: image_size }} 1x, {{ image | img_url: image_size, scale: 2 }} 2x"
                                src="{{ image | img_url: image_size }}"
